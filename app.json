{
  "expo": {
    "name": "Transl",
    "slug": "Transl",
    "scheme": "Transl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/transl_icon.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/transl_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jackfr8st69.Transl"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b5d3cd78-fe5e-46b3-ad41-79bdc724f488"
      }
    }
  }
}
